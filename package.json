    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "prod:build:weapp": "taro build --type weapp --mode prod",
    "test:build:weapp": "taro build --type weapp --mode test",
    "prod:build:weapp-enforce": "cross-env TARO_APP_PLATFORM=ENFORCE taro build --type weapp --mode prod",
    "test:build:weapp-enforce": "cross-env TARO_APP_PLATFORM=ENFORCE taro build --type weapp --mode test",
    "dev:build:weapp": "taro build --type weapp --mode dev",
    "dev:weapp": "npm run dev:build:weapp -- --watch",
    "dev:weapp-enforce": "cross-env TARO_APP_PLATFORM=ENFORCE npm run dev:build:weapp -- --watch",
    "test:weapp": "npm run test:build:weapp -- --watch",
    "prod:weapp": "npm run prod:build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
